{
  "name": "fadverse-web",
  "version": "1.0.0",
  "description": "A simple IoT platform - Web Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "npm run prestart-dev && DEBUG=fadverse:* nodemon server.js",
