  "bugs": {
    "url": "https://github.com/FAD95/Curso_Avanzado_de_Node.js/issues"
  },
  "homepage": "https://github.com/FAD95/Curso_Avanzado_de_Node.js#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "nodemon": "^2.0.4",
    "standard": "^14.3.4",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chart.js": "^2.9.3",
    "debug": "^4.1.1",
    "express": "^4.17.1",
